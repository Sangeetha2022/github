{
    "feature-name": "gepFileManager",
    "type": "external",
    "version": "1.0.0",
    "description": "\"Generic file uploads/downloads/management\"",
    "author": "gepinfo.@gmail.com",
    "repo":{
        "type": "github",
        "location": "https://github.com/dan-castillo/gepFileManager.git",
        "branch" : "main"

    },
    "license":{
        "type": "OSS", 
        "name": "Apache 2",
        "price": "0",
        "term": "",
        "comments": "best effort support"
    },
    "server-properties":{
        "runtime": "nodejs",
        "runtime-version": "14.0.0",
        "dev-framework": "express",
        "dev-framework-version": "4.7.1",
        "datastore": "mongo",
        "datastore-version": "4.1.1",
        "flows":[
            {
                "name": "gepFileUpload",
                "api": "/eeee/xxxx/",
                "api-method": "post"

            },
            {
                "name": "gepFileDownload",
                "api": "/eeee/xxxx/",
                "api-method": "get"

            }

        ]
    },
    "client-properties":{
      "framework": "angular",
      "framework-version": "12.0.0",
      "flows":[
        {
            "name": "gepFileUpload",
            "route": "/hkkjhk/hkhkk",
            "component-name": "upload-screen"

        },
        {
            "name": "gepFileDownload",
            "route": "/hkkjhk/hkhkk",
            "component-name": "download-screen"


        }

    ]

    }
  }
  
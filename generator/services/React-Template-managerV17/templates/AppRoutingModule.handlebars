{{#each importComponent}}
import {{classname}} from "./Components/{{classname}}/{{classname}}";
{{/each}}

var routes: any = [
  {{#each componentPath}}
    {
        path: "{{path}}",
        name: "{{component}}",
        component: {{component}},
        {{#if isAuthImport }} 
            layout: "/auth",
        {{else}}
            layout: "/user",
        {{/if}}
    },
  {{/each}}
]

export default routes;

